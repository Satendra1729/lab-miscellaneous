  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "HelloWorld",
      "request": "launch",
      "mainClass": "testJava.HelloWorld",
      "projectName": "lab-miscellaneous_c7d40119",
      "env": {
        "TEST_ENV": "This is test env"
      }
    },
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/MyFunction/test/MyFunction.Tests/bin/Debug/net6.0/MyFunction.Tests.dll",
      "args": [],
      "cwd": "${workspaceFolder}/MyFunction/test/MyFunction.Tests",
      "console": "internalConsole",
      "stopAtEntry": false,
      "justMyCode": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "C/C++: g++ build and debug active file",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++ build active file"
    }
  ]
}
